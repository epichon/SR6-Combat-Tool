    <object type="SR6PROGRAM" id="z381">
        <attribute name="shortdesc" type="string">Gain 1 Edge for Matrix Searches.</attribute>
        <attribute name="reference" type="string">SR6 184</attribute>
        <attribute name="programuuid" type="UUID">641DBEE5-184C-4854-8792-22C96AA0477F</attribute>
