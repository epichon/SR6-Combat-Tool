        <attribute name="shortdesc" type="string">-2 to DR of target.</attribute>
        <attribute name="reference" type="string">SR6 184</attribute>
        <attribute name="programuuid" type="UUID">8DED39FF-133E-48E6-B79B-DED435504E6F</attribute>
        <attribute name="name" type="string">Exploit</attribute>
        <attribute name="maxlevel" type="int16">0</attribute>
