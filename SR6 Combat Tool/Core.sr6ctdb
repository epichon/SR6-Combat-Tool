        <attribute name="reference" type="string">SR6 141</attribute>
        <attribute name="range" type="int16">1</attribute>
        <attribute name="name" type="string">Mystic Armor</attribute>
        <attribute name="longdesc" type="string">Your magic fills the astral form of the target, hardening it and making it better able to absorb damage. Roll Sorcery + Magic and add net hits to target’s Defense Rating when in astral combat or against mana-based Combat spells.</attribute>
        <attribute name="duration" type="int16">2</attribute>
        <attribute name="drain" type="int16">3</attribute>
        <attribute name="damage" type="int16">0</attribute>
        <attribute name="category" type="int16">5</attribute>
    </object>
